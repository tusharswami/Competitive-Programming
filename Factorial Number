//Factorial of a Number
#include <stdio.h>

int main()
{
    int num, Ans;
    printf("Enter a Number");
    scanf("%d", &num);
    Ans = factorial(num);
    printf("Factorial of %d is %d",num, Ans);
    return 0;
}

int factorial(int a){
    if(a == 1)
        return 1;
    return a * factorial(a - 1);
}

